        <randomWalk>6.4e-08</randomWalk>
        <biasCorrelationTime>6</biasCorrelationTime>
        <magTopic>/mag</magTopic>
    </plugin>

    <plugin name='barometer_plugin' filename='libgazebo_barometer_plugin.so'>
      <robotNamespace/>
